{
  "name": "tabbed",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "start": "run-s start-chrome",
    "start-chrome": "cross-env NODE_ENV=development TARGET=chrome REACT_APP_TARGET=chrome run-s clean copy-public manifest build:tailwind && run-p start:*",
    "start:app": "cross-env BROWSER=none ts-node -P scripts/tsconfig.json scripts/start-cra",
    "start:tailwind": "npx tailwindcss --postcss -i ./src/styles/index.css -o ./src/index.css --watch",
    "start:background": "ts-node -P scripts/tsconfig.json esbuild.ts",
    "build": "run-s build-chrome",
    "build-chrome": "cross-env NODE_ENV=production TARGET=chrome REACT_APP_TARGET=chrome run-s clean build:tailwind build:app build:background build:manifest build:post-build",
    "build:app": "craco build",
    "build:tailwind": "npx tailwindcss -m --postcss -i ./src/styles/index.css -o ./src/index.css",
    "build:background": "ts-node -P scripts/tsconfig.json esbuild.ts",
    "build:manifest": "run-s manifest",
    "build:post-build": "ts-node -P scripts/tsconfig.json scripts/post-build",
    "eject": "react-scripts eject",
    "clean": "ts-node -P scripts/tsconfig.json scripts/clean",
    "copy-public": "ts-node -P scripts/tsconfig.json scripts/copy-public",
    "manifest": "ts-node -P scripts/tsconfig.json scripts/create-manifest",
    "format": "run-p format:*",
    "format:packagejson": "sort-package-json",
    "format:prettier": "prettier --write \"**/*.{ts,tsx,js,jsx}\"",
    "test": "run-s test:*",
    "test:jest": "craco test",
    "test:ts": "tsc --noEmit",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "analyze": "source-map-explorer 'build/static/js/*'",
    "pack-mac": "/Applications/Google\\ Chrome.app/Contents/MacOS/Google\\ Chrome --pack-extension=build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "dependencies": {
    "@headlessui/react": "^1.7.13",
    "@hello-pangea/dnd": "^16.2.0",
    "@mdi/js": "^7.2.96",
    "@mdi/react": "^1.6.1",
    "@popperjs/core": "^2.11.7",
    "@react-aria/focus": "^3.11.0",
    "bowser": "^2.11.0",
    "class-transformer": "^0.5.1",
    "classnames": "^2.3.2",
    "copy-to-clipboard": "^3.3.3",
    "date-fns": "^2.29.3",
    "framer-motion": "^10.10.0",
    "fuse.js": "^6.6.2",
    "jotai": "^2.0.3",
    "lodash": "^4.17.21",
    "loglevel": "^1.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hotkeys-hook": "^4.3.8",
    "react-popper": "^2.3.0",
    "react-scripts": "^5.0.1",
    "reflect-metadata": "^0.1.13",
    "uuid": "^9.0.0",
    "web-vitals": "^3.3.0",
    "webextension-polyfill": "^0.10.0"
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/chrome": "^0.0.227",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.192",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.32",
    "@types/react-dom": "^18.0.11",
    "@types/uuid": "^9.0.1",
    "@types/webextension-polyfill": "^0.10.0",
    "autoprefixer": "^10.4.14",
    "craco-esbuild": "^0.5.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "esbuild": "^0.17.15",
    "jest-chrome": "^0.8.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-import": "^15.1.0",
    "prettier": "^2.8.7",
    "sort-package-json": "^2.4.1",
    "source-map-explorer": "^2.5.3",
    "tailwindcss": "^3.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
